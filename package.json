  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Pinka/mimic/issues"
  },
  "homepage": "https://pinka.github.io/mimic",
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "husky": "^1.3.1",
    "lint-staged": "^11.0.0",
    "prettier": "2.3.0"
  },
  "lint-staged": {
    "*.{css,scss}": [
      "prettier --write"
